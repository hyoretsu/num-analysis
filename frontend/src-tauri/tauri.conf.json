{
	"$schema": "https://schema.tauri.app/config/2",
	"app": {
		"security": {
			"csp": null
		},
		"windows": [
			{
				"height": 600,
				"title": "num-analysis",
				"width": 800
			}
		]
	},
	"build": {
		"beforeBuildCommand": "bun run build",
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:3000",
		"frontendDist": "../out"
	},
	"bundle": {
		"active": true,
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"targets": "all"
	},
	"identifier": "com.hyoretsu.numanalysis",
	"productName": "num-analysis",
	"version": "1.0.0"
}

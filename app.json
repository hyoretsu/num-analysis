{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./src/assets/adaptive-icon.png"
      },
      "package": "com.hyoretsu.numanalysis"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./src/assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.hyoretsu.numanalysis",
      "supportsTablet": true
    },
    "name": "NumAnalysis",
	"newArchEnabled": true,
    "orientation": "portrait",
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "scheme": "numanalysis",
    "slug": "NumAnalysis",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./src/assets/splash.png",
      "resizeMode": "contain"
    },
    "version": "1.0.0"
  }
}
